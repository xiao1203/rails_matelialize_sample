# encoding: utf-8
create_table :crypto_currencies, force: :cascade, options: 'ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci' do |t|
  t.string   :name,   null: true, comment: '名前'
  t.string   :address,   null: true, comment: 'アドレス'
end
